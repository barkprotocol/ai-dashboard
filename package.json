{
  "name": "ai-dashboard",
  "description": "The dashboard offers an intuitive interface for managing token trades, analyzing real-time price data, and gaining actionable insights powered by AI",
  "version": "0.1.0-beta",
  "private": true,
  "scripts": {
    "vercel-build": "NODE_OPTIONS=--max_old_space_size=2048 pnpm prisma generate && next build",
    "dev": "NODE_NO_WARNINGS=1 next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:reset": "pnpm prisma generate && pnpm prisma migrate reset && pnpm prisma db push",
    "generate": "pnpm prisma generate",
    "migrate": "pnpm prisma migrate deploy",
    "dev:up-build": "docker compose up --build -d",
    "dev:up": "docker compose up -d",
    "format": "prettier --write \"**/*.{ts,tsx,md,json,js,jsx}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,md,json,js,jsx}\""
  },
